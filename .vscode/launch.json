{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Current",
      "program": "${file}",
      "preLaunchTask": "npm: build",
      "outFiles": ["${workspaceFolder}/js/**/*.js"],
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example",
      "program": "${workspaceFolder}/example/index.ts",
      "preLaunchTask": "npm: build",
      "outFiles": ["${workspaceFolder}/js/**/*.js"],
    }
  ]
}